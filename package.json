{
  "name": "reviews",
  "version": "1.0.0",
  "description": "Review component server and client",
  "author": "Shoji",
  "license": "N/A",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "reset_db": "mysql -h 54.193.74.145 -uroot < ./db/lego_schema.sql",
    "seed_db": "node ./db/seed/seeder.js",
    "nodemon": "nodemon ./server/index.js",
    "start": "webpack --config webpack.prod.js && node ./server/index.js",
    "server": "concurrently -k \"npm run nodemon\" \"npx webpack -w --config webpack.dev.js\"",
    "docker_me": "sleep 10 && \"npm start\"",
    "circleci": "webpack && npm run seed_db",
    "ci:test": "jest ./client --ci --runInBand --detectOpenHandles --forceExit",
    "test": "jest",
    "test:w": "jest . --watch",
    "lint": "eslint ."
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "isomorphic-fetch": "^2.2.1",
    "jest-fetch-mock": "^3.0.3",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.9.4",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-html-attrs": "^3.0.4",
    "babel-preset-airbnb": "^5.0.0",
    "brotli-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^4.0.0",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor.git",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "html-webpack-plugin": "^4.3.0",
